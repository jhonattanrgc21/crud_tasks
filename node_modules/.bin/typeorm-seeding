#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/nod" ]; then
  "$basedir/nod"  "$basedir/../typeorm-seeding/dist/cli.js" "$@"
  ret=$?
else 
  nod  "$basedir/../typeorm-seeding/dist/cli.js" "$@"
  ret=$?
fi
exit $ret
